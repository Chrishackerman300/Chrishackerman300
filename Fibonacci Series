/*............................FIBONACCI SERIES...........................*/
#include<iostream>
#include<conio.h>
using namespace std;

int main(){
	
	//Var
	int i, num, n=1, nanterior= 0, aux;
  
	cout<<"Ingrese la serie de numeros a calcular: ";
	cin>> num;
	
	for(i=0; i<= num; i++)
	{
		cout<<" "<< n;
		aux= n;
		n += nanterior;
		nanterior= aux;
	}
  
	getch();
	return 0;
}
